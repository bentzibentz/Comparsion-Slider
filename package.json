{
  "name": "pineapple-comparsions-slider",
  "version": "0.1.2",
  "description": "Pineapple Comparsion Slider",
  "main": "dist/comparsions-slider.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/comparsions-slider.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "0.6.1",
    "@types/hammerjs": "2.0.35",
    "hammerjs": "2.0.8"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "21.1.1",
    "jest": "21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bentzibentz/Pineapple-Comparsion-Slider"
  },
  "author": "Fabian bentz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bentzibentz/Pineapple-Comparsion-Slider"
  },
  "homepage": "https://github.com/bentzibentz/Pineapple-Comparsion-Slider",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
